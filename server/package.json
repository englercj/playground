{
  "name": "@pixi/playground-server",
  "version": "1.0.0",
  "description": "Server application for the pixi playground.",
  "author": "Chad Engler <chad@pantherdev.com>",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/englercj/playground#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/englercj/playground.git"
  },
  "bugs": {
    "url": "https://github.com/englercj/playground/issues"
  },
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json --outDir dist",
    "clean": "rimraf dist",
    "dev": "ts-node src/migrate.ts | bunyan && nodemon | bunyan",
    "start": "pm2 start --env production",
    "restart": "pm2 reload all",
    "migrate-db": "NODE_ENV=production node migrate.js | bunyan",
    "test": "mocha -r ts-node/register test/unit/**/*.ts"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/chai": "^4.1.4",
    "@types/http-codes": "^1.0.1",
    "@types/mocha": "^5.2.5",
    "@types/restify": "^7.2.4",
    "@types/sequelize": "^4.27.25",
    "@types/nanoid": "^1.2.0",
    "@types/supertest": "^2.0.5",
    "@types/umzug": "^2.2.0",
    "@types/restify-cors-middleware": "^1.0.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "sqlite3": "^4.0.2",
    "supertest": "^3.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "dotenv": "^6.0.0",
    "http-codes": "^1.0.0",
    "mysql2": "^1.6.1",
    "reflect-metadata": "^0.1.12",
    "restify": "^7.2.1",
    "restify-cors-middleware": "^1.1.1",
    "sequelize": "^4.38.0",
    "sequelize-typescript": "^0.6.6",
    "nanoid": "^1.2.2",
    "umzug": "^2.1.0"
  },
  "engines": {
    "node": "^10.9.0"
  }
}
